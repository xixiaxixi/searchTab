{
  "manifest_version": 3,
  "name": "Search New Tab",
  "version": "0.4.0",
  "description": "Replaces the default new tab page with a custom one",
  "permissions": [
    "tabs",
    "storage",
    "bookmarks",
    "history",
    "favicon"
  ],
  "icons": {
    "16": "favicon.ico",
    "48": "favicon.ico",
    "128": "favicon.ico"
  },
  "chrome_url_overrides": {
    "newtab": "src/index.html"
  },
  "default_locale": "zh_CN",
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "**/*",
        "*"
      ],
      "use_dynamic_url": true
    }
  ]
}
